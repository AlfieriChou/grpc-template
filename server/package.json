{
  "name": "grpc-server-template",
  "version": "1.0.0",
  "description": "Node gRPC server template for typescript.",
  "scripts": {
    "server": "node ./build/server.js",
    "proto": "node ./bin/proto.js",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "build": "tsc -p tsconfig.json",
    "build:format": "yarn run build && yarn run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": "https://github.com/AlfieriChou/grpc-template",
  "author": "AlfieriChou <alfierichou@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "11.12.0",
    "grpc-tools": "1.7.2",
    "grpc_tools_node_protoc_ts": "2.5.0",
    "husky": "1.3.1",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "rimraf": "2.6.3",
    "shelljs": "0.8.5",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.4000"
  },
  "dependencies": {
    "grpc": "1.19.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.8.0"
  }
}
