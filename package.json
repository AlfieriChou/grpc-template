{
  "name": "grpc-template",
  "version": "1.0.0",
  "description": "Node gRPC template for typescript.",
  "scripts": {
    "server": "node ./build/server.js",
    "client": "node ./build/client.js",
    "js:client": "node ./src/client.js",
    "proto": "node ./bin/proto.js",
    "cert": "sudo sh ./script/gen-cert.sh",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format": "prettier --write \"**/*.ts\" \"**/*.js\"",
    "build": "tsc -p tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": "https://github.com/AlfieriChou/grpc-template",
  "author": "AlfieriChou <alfierichou@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/google-protobuf": "3.2.7",
    "@types/node": "11.10.4",
    "grpc-tools": "1.7.1",
    "grpc_tools_node_protoc_ts": "2.5.0",
    "husky": "1.3.1",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "rimraf": "2.6.3",
    "shelljs": "0.8.3",
    "ts-node": "8.0.2",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "google-protobuf": "3.7.0",
    "grpc": "1.19.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.8.0"
  }
}
